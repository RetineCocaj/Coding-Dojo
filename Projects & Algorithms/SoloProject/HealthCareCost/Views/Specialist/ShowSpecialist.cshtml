@model Specialist

<h2 class="mb-5 ml-2" style="color:#199CA6;">@Model.FirstName @Model.LastName</h2>
<h4 class=" ml-2">Specializations</h4>
<ul class="list-group col-6 mb-3">
    @foreach (var item in ViewBag.SpecialistSpecializations)
    {
        if(item.SpecialistId == Model.SpecialistId)
        {
            <li class="list-group-item mb-2">@item.Specialization.Name</li>
        }
    }
</ul>
<h4 class=" ml-2">Contact</h4>
<ul class="list-group col-6">
    <li class="list-group-item mb-2">Email: @Model.Email</li>
    <li class="list-group-item mb-2">Telephone: @Model.Telephone</li>
</ul>